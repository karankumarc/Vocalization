<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="voice.text"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:icon="@drawable/vocal"
        android:label="@string/app_name" >
        <activity
            android:name="voice.text.Vocalization"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="voice.text.WebViewClass" />
        <activity android:name="voice.text.TextToSpeechAct" />
        <activity android:name="voice.text.VoiceRecognition" />
    </application>

</manifest>